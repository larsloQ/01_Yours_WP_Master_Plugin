{
  "name": "yours-block-builder",
  "version": "0.1.0",
  "description": "yours-block-builder Gutenberg block example based on https://github.com/florianbrinkmann/t3n-gutenberg-block.git and others",
  "repository": {
    "type": "git",
    "url": "yours-block-builder"
  },
  "scripts": {
    "build:production": "webpack --config webpack.config.production.js",
    "build:dev": "webpack",
    "watch": "webpack --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors"
  },
  "author": "larslo",
  "license": "GPLv2 or later",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/runtime": "^7.3.1",
    "@wordpress/babel-plugin-import-jsx-pragma": "^1.1.3",
    "@wordpress/babel-preset-default": "^3.0.2",
    "@wordpress/block-library": "^2.2.13",
    "@wordpress/blocks": "^6.0.6",
    "@wordpress/components": "^7.0.6",
    "@wordpress/data": "^4.2.1",
    "@wordpress/e2e-test-utils": "file:packages/e2e-test-utils",
    "@wordpress/e2e-tests": "file:packages/e2e-tests",
    "@wordpress/element": "^2.1.9",
    "@wordpress/i18n": "^3.1.1",
    "@wordpress/jest-preset-default": "^3.0.3",
    "@wordpress/list-reusable-blocks": "^1.1.19",
    "autoprefixer": "^8.6.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint": "^5.12.1",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-jest": "^22.2.1",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-cli": "^5.0.0",
    "postcss-loader": "^2.1.6",
    "react": "^16.7.0",
    "react-test-renderer": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.1.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "radium": "^0.25.1",
    "styled-jsx": "^3.2.0"
  },
  "npmPackageJsonLintConfig": {
    "extends": "@wordpress/npm-package-json-lint-config",
    "rules": {
      "description-format": [
        "error",
        {
          "requireCapitalFirstLetter": true,
          "requireEndingPeriod": true
        }
      ],
      "require-publishConfig": "error",
      "valid-values-author": [
        "error",
        [
          "The WordPress Contributors"
        ]
      ],
      "valid-values-publishConfig": [
        "error",
        [
          {
            "access": "public"
          }
        ]
      ]
    }
  }
}
